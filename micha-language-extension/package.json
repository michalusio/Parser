{
    "name": "m-language-extension",
    "displayName": "M-Language Extension",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.59.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "m-lang",
            "aliases": ["M-Language", "m-lang"],
            "extensions": [".mlang"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "m-lang",
            "scopeName": "source.mlang",
            "path": "./syntaxes/m-lang.tmLanguage.json"
        }]
    }
}